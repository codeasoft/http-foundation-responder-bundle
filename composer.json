{
  "name": "tuzex/responder-bundle",
  "description": "Intergation of smart responder based on the Symfony HTTP Foundation to Symfony 4.3 and higher",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.0.0",
      "ext-intl": "1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Tuzex\\Bundle\\Responder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tuzex\\Bundle\\Responder\\Test\\": "tests"
    }
  },
  "require": {
    "php": "8.*",
    "symfony/framework-bundle": "4.3.*|4.4.*|5.*",
    "symfony/twig-bundle": "4.3.*|4.4.*|5.*",
    "tuzex/responder": "0.2.*"
  },
  "require-dev": {
    "phpstan/phpstan": "0.12.*",
    "phpstan/phpstan-phpunit": "0.12.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "9.0.*"
  }
}
